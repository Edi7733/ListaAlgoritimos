//31. Conversão de quilômetros para milhas: Converta uma distância em km para milhas.
programa {
  funcao inicio() {
    real quilom, milhas
    escreva("Informe quantos quilometros:" )
    leia(quilom)
    milhas=quilom * 1000
    escreva(quilom," quilometros equivale a ", milhas, " milhas.")

  }
}
