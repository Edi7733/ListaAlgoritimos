programa { //10. Contagem Regressiva: Exiba uma contagem regressiva de 10 a 1.

  funcao inicio() {
    inteiro nun // Variavel "nun"
    nun = 10  //"nun" começa a contagem regressiva com o valor de 10
    enquanto (nun > - 1){ // Enquanto "nun" for maior que - 1 é para fazer
      escreva(nun,"\n") //Apresenta o resultado ao úsuario
      nun = nun - 1 //"nun" recebe - 1 para criar o loop regressivo


    }
    
  }
}
